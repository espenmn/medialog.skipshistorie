<?xml version="1.0"?>
<portlets
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plone">
  <!-- Extra portlets here  -->

  <portlet
    addview="medialog.skipshistorie.portlets.Metadata"
    title="MetaData"
    description="A portlet "
    i18n:attributes="title title_metadata;
                     description description_metadata">

    <!-- This will enable the portlet for right column,
    left column and the footer too.
    -->
    <for interface="plone.app.portlets.interfaces.IColumn" />

    <!--
    This will enable the portlet in the dashboard.
    -->
    <!--<for interface="plone.app.portlets.interfaces.IDashboard" />-->

  </portlet>

  <portlet
    addview="plone.app.portlets.portlets.navigation.AddForm"
    title="Innhold"
    description="A special navigationportlet"
    i18n:attributes="title title_metadata;
                     description description_metadata">

    <for interface="plone.app.portlets.interfaces.IColumn" />
  </portlet>

</portlets>
